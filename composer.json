{
    "name": "strikebit/json-to-php",
    "description": "Utility for creating PHP classes from JSON",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Benson",
            "email": "strikebit.io@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3.1",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^4.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Strikebit\\Util\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strikebit\\Util\\Tests\\": "tests/"
        }
    }
}
